{
  "name": "fxscript-vscode",
  "displayName": "FXScript",
  "description": "Syntax highlighting and IntelliSense for FXScript.",
  "version": "0.1.0",
  "publisher": "nitwhiz",
  "repository": "https://github.com/nitwhiz/fxscript-vscode",
  "engines": {
    "vscode": "^1.100.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "fxscript",
        "aliases": [
          "FXScript"
        ],
        "extensions": [
          ".fx",
          ".fxs",
          ".fxt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fxscript",
        "scopeName": "source.fx",
        "path": "./syntaxes/fxscript.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "build": "tsc -p ./",
    "package": "vsce package -o fxscript.vsix",
    "vscode:install:mac": "'/Applications/Visual Studio Code.app/Contents/Resources/app/bin/code' --install-extension fxscript.vsix",
    "vscode:install": "code --install-extension fxscript.vsix"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@types/vscode": "^1.100.0",
    "typescript": "^5.4.0",
    "@vscode/vsce": "^3.7.1"
  },
  "dependencies": {}
}
